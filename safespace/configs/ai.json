{
  "models": {
    "accident_detection": {
      "type": "yolo",
      "path": "AI Layer/Models/accident_detection_model.pt",
      "confidence": 0.7,
      "enabled": true
    },
    "vehicle_detection": {
      "type": "yolo",
      "path": "AI Layer/Models/vehicle_detection_model.pt",
      "confidence": 0.5,
      "enabled": true
    }
  },

  "tracking": {
    "algorithm": "botsort",
    "persist": true,
    "confidence": 0.5,
    "iou_threshold": 0.5,
    "max_age": 30,
    "history_length": 50
  },

  "processing": {
    "frame_skip": 1,
    "batch_size": 1,
    "use_gpu": true,
    "history_size": 10
  },

  "alerts": {
    "enabled": true,
    "cooldown_seconds": 5.0,
    "min_confidence": 0.5,
    "trigger_classes": [
      "accident",
      "crash",
      "collision",
      "fire"
    ]
  },

  "debug": {
    "log_detections": false,
    "save_frames": false,
    "visualize": false
  }
}